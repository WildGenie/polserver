var a := 4, b := 7, c := 3, d := 5;

// If ternary operator precedence is ______ than comparison
                        //   LOWER (correct)              HIGHER (incorrect)
print( a > b ? c : d ); //   5*   (a > b) ? c : d         1   a > (b ? c : d)
print( a ? b : c > d ); //   7    a ? b : (c > d)         1*   (a ? b : c) > d

// * most intuitive